package hw2;

import java.util.Scanner;

public class TwentyOne {

	public static void main(String[] args) {
		// 사용자로부터 한 숫자를 입력받아, 입력받은 숫자 이하의 소수들의 합을 구하는 프로그램을 작성하시오.
		for(int i = 1; i <= 100; i++) {

			System.out.println(i + " 의 약수 : ");

			for(int j = 1; j <= i; j++) {

				if(i % j == 0) {
					System.out.println(j);
				}
			}
		}
	}
}


